<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="sample.kingja.loadsir">

    <application
        android:name=".App"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>

        </activity>
        <activity android:name=".target.NormalActivity"/>
        <activity android:name=".target.ConvertorActivity"/>
        <activity android:name=".target.FragmentSingleActivity"/>
        <activity android:name=".target.ViewTargetActivity"/>
        <activity android:name=".target.DefaultCallbackActivity"/>
        <activity android:name=".target.MultiFragmentWithViewPagerActivity"/>
        <activity android:name=".target.MultiFragmentActivity"/>
        <activity android:name=".target.PlaceholderActivity"/>
        <activity android:name=".target.AnimateActivity"/>
        <activity android:name=".target.KeepTitleFragmentActivity"/>
        <activity android:name=".target.BestPracticesActivity"/>
        <activity android:name=".target.LottieActivity"/>
        <activity
            android:name=".target.KeepTitleActivity"
            android:theme="@style/NoTitle"/>
        <activity android:name=".target.ConstraintLayoutActivity"/>
    </application>

</manifest>